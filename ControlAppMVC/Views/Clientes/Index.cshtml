@model IEnumerable<ControlAppMVC.Models.Cliente>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<ul>
@foreach (var cliente in Model)
{
    <li>@cliente.Id</li>
    <li>@cliente.Name</li>
    <li>@cliente.Email</li>
    <li>@cliente.PhoneNumber</li>
}
</ul>
<h2>Index</h2>

